{
  "name": "cacau-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:start": "nodemon --exec ts-node-dev ./src/server.ts",
    "start": "node dist/server.js",
    "build:fast": "tsc --noEmitOnError --incremental --skipLibCheck",
    "knex:migrate": "knex --knexfile ./src/database/knex/index.ts migrate:latest",
    "knex:rollback": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback --all",
    "knex:seed": "knex --knexfile ./src/database/knex/index.ts seed:run"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.0.2",
    "nodemon": "^3.1.4",
    "ts-node-dev": "^2.0.0"
  }
}
